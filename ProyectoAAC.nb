     RowBox[{"(", 
      RowBox[{
       FractionBox["4", 
        RowBox[{
         RowBox[{"8", "k"}], "+", "1"}]], "-", 
       FractionBox["2", 
        RowBox[{
         RowBox[{"8", "k"}], "+", "4"}]], "-", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"8", "k"}], "+", "5"}]], "-", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"8", "k"}], "+", "6"}]]}], ")"}]}]}]}], 
