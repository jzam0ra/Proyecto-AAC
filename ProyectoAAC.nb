6d6e642bc5de"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"Log", "(", "2", ")"}], "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", "1"}], "\[Infinity]"], 
    RowBox[{
     FractionBox["1", 
      RowBox[{"k", " ", 
       SuperscriptBox["2", "k"]}]], "."}]}]}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.9094342217569504`*^9, 3.9094342587662864`*^9}, {
