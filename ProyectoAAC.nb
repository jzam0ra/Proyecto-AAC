Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SecureVertexQ", "[", 
    RowBox[{"r_", ",", "b_"}], "]"}], "[", 
   RowBox[{"G_", ",", "v_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Or", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"MemberQ", "[", 
      RowBox[{"r", ",", "v"}], "]"}], "&&", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{
        RowBox[{"neighbors", "[", 
         RowBox[{"G", ",", "v"}], "]"}], "\[Intersection]", "r"}], "]"}], " ",
       ">", 
      RowBox[{"Length", "[", 
       RowBox[{
        RowBox[{"neighbors", "[", 
         RowBox[{"G", ",", "v"}], "]"}], "\[Intersection]", "b"}], "]"}]}]}], 
    ",", 
    RowBox[{
     RowBox[{"MemberQ", "[", 
      RowBox[{"b", ",", "v"}], "]"}], "&&", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{
        RowBox[{"neighbors", "[", 
         RowBox[{"G", ",", "v"}], "]"}], "\[Intersection]", "b"}], "]"}], ">", 
      RowBox[{"Length", "[", 
       RowBox[{
        RowBox[{"neighbors", "[", 
         RowBox[{"G", ",", "v"}], "]"}], "\[Intersection]", "r"}], 
       "]"}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
