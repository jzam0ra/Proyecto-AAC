     RowBox[{"MemberQ", "[", 
      RowBox[{"b", ",", "y"}], "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"MemberQ", "[", 
      RowBox[{"b", ",", "x"}], "]"}], "&&", 
     RowBox[{"MemberQ", "[", 
      RowBox[{"r", ",", "y"}], "]"}]}]}], "\[IndentingNewLine]", 
