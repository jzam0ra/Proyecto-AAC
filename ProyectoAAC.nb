Cell[TextData[{
 "The function ",
 Cell[BoxData[
 "flip"], "MCode",ExpressionUUID->"c0ef22ee-3d9c-440a-8f61-042c5cf1111c"],
 " changes the color of a vertex."
}], "Text",ExpressionUUID->"54516e11-669f-4df7-9810-db53582ecbb1"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"flip", "[", 
    RowBox[{"r_", ",", "b_"}], "]"}], "[", "v_", "]"}], ":=", 
  RowBox[{"If", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"MemberQ", "[", 
     RowBox[{"r", ",", "v"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"DeleteCases", "[", 
       RowBox[{"r", ",", "v"}], "]"}], ",", 
      RowBox[{"Append", "[", 
       RowBox[{"b", ",", "v"}], "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Append", "[", 
       RowBox[{"r", ",", "v"}], "]"}], ",", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{"b", ",", "v"}], "]"}]}], "}"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellLabel->"In[13]:=",ExpressionUUID->"93a244f9-c148-4067-9f96-ad29c337cdfc"],

