Cell["Unfriendly Coloring a Graph", "Section",ExpressionUUID->"4b9406be-3c56-4d65-b3fb-62b429177140"],

Cell[TextData[{
 "The following short program produces an unfriendly coloring of any \
2-colored graph starting with the color partition ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{"red", ",", "blue"}], "}"}]], "MCode",ExpressionUUID->
  "b9c66f96-a577-4dda-a02f-bc1620ea97c2"],
 ". "
}], "Text",
 CellChangeTimes->{{3.8384694875741673`*^9, 
  3.8384695429789762`*^9}},ExpressionUUID->"b9e6b1da-4c33-4e7e-8296-\
8f98f7f5609e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"UnfriendlyColor", "[", 
    RowBox[{"red_", ",", "blue_"}], "]"}], "[", "G_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"r", ",", "b", ",", "sv", ",", "v"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"r", ",", "b"}], "}"}], "=", 
      RowBox[{"{", 
       RowBox[{"red", ",", "blue"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"sv", "=", 
      RowBox[{
       RowBox[{"SecureVertices", "[", 
        RowBox[{"r", ",", "b"}], "]"}], "[", "G", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"sv", "\[NotEqual]", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"v", "=", 
         RowBox[{"First", "@", "sv"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"r", ",", "b"}], "}"}], "=", 
         RowBox[{
          RowBox[{"flip", "[", 
           RowBox[{"r", ",", "b"}], "]"}], "[", "v", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"sv", "=", 
         RowBox[{
          RowBox[{"SecureVertices", "[", 
           RowBox[{"r", ",", "b"}], "]"}], "[", "G", "]"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"r", ",", "b"}], "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellLabel->"In[25]:=",ExpressionUUID->"3e8b3404-58ae-425c-b5d7-286203e88f56"],

