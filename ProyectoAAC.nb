        RowBox[{
         RowBox[{"p", "(", "k", ")"}], 
         SuperscriptBox["b", 
          RowBox[{"n", "-", "1", "-", 
           RowBox[{"c", " ", "k"}]}]]}], 
        RowBox[{"q", "(", "k", ")"}]]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"mod", " ", "1"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", "0"}], 
          RowBox[{"\[LeftFloor]", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}], "/", "c"}], "\[RightFloor]"}]], 
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"p", "(", "k", ")"}], 
            SuperscriptBox["b", 
             RowBox[{"n", "-", "1", "-", 
              RowBox[{"c", " ", "k"}]}]]}], 
           RowBox[{"q", "(", "k", ")"}]], 
          RowBox[{"(", 
           RowBox[{"mod", " ", "1"}], ")"}]}]}], "+", " ", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", 
           RowBox[{
            RowBox[{"\[LeftFloor]", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"n", "-", "1"}], ")"}], "/", "c"}], "\[RightFloor]"}], 
            "+", "1"}]}], "\[Infinity]"], 
         FractionBox[
          RowBox[{
           RowBox[{"p", "(", "k", ")"}], 
           SuperscriptBox["b", 
            RowBox[{"n", "-", "1", "-", 
             RowBox[{"c", " ", "k"}]}]]}], 
          RowBox[{"q", "(", "k", ")"}]]}]}], "}"}], 
      RowBox[{"(", 
       RowBox[{"mod", " ", "1"}], ")"}]}], "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", "0"}], 
          RowBox[{"\[LeftFloor]", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}], "/", "c"}], "\[RightFloor]"}]], 
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"p", "(", "k", ")"}], 
            RowBox[{"(", 
             RowBox[{"mod", " ", 
              RowBox[{"q", "(", "k", ")"}]}], ")"}], 
            RowBox[{
             SuperscriptBox["b", 
              RowBox[{"n", "-", "1", "-", 
               RowBox[{"c", " ", "k"}]}]], "(", 
             RowBox[{"mod", " ", 
              RowBox[{"q", "(", "k", ")"}]}], ")"}]}], 
           RowBox[{"q", "(", "k", ")"}]], 
          RowBox[{"(", 
           RowBox[{"mod", " ", "1"}], ")"}]}]}], "+", " ", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", 
           RowBox[{
            RowBox[{"\[LeftFloor]", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"n", "-", "1"}], ")"}], "/", "c"}], "\[RightFloor]"}], 
            "+", "1"}]}], "\[Infinity]"], 
         FractionBox[
          RowBox[{
           RowBox[{"p", "(", "k", ")"}], 
           SuperscriptBox["b", 
            RowBox[{"n", "-", "1", "-", 
             RowBox[{"c", " ", "k"}]}]]}], 
          RowBox[{"q", "(", "k", ")"}]]}]}], "}"}], 
      RowBox[{"(", 
       RowBox[{"mod", " ", "1"}], ")"}]}]}]}]}], 
  TraditionalForm]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.909432786895316*^9, 3.9094330141474786`*^9}, {
