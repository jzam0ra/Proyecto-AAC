      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", "z"], 
       RowBox[{
        FractionBox[
         RowBox[{"Log", "(", "z", ")"}], 
         RowBox[{"1", "-", "z"}]], 
        RowBox[{"\[DifferentialD]", "z"}]}]}]}], " ", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        RowBox[{"Log", "(", "z", ")"}]}], 
       RowBox[{"Log", "(", 
        RowBox[{"1", "-", "z"}], ")"}]}], "-", 
      RowBox[{
       SubscriptBox["Li", "2"], "(", 
       RowBox[{"1", "-", "z"}], ")"}], "+", 
      RowBox[{
       SubscriptBox["Li", "2"], "(", "1", ")"}]}]}]}], ","}], 
  TraditionalForm]], "DisplayFormulaNumbered",
