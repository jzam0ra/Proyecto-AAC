Cell[BoxData[
 RowBox[{
  RowBox[{"HexDigitsPi", "[", 
   RowBox[{"n_", ",", "x_"}], "]"}], ":=", 
  RowBox[{"(", "\n", 
   RowBox[{
    RowBox[{"s1", "=", "0"}], ";", "\n", 
    RowBox[{"s4", "=", "0"}], ";", "\n", 
    RowBox[{"s5", "=", "0"}], ";", "\n", 
    RowBox[{"s6", "=", "0"}], ";", "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", "0"}], ",", 
      RowBox[{"k", "\[LessEqual]", "n"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s1", "=", 
       RowBox[{"s1", "+", 
        RowBox[{"Divide", "[", 
         RowBox[{
          RowBox[{"ExpModK", "[", 
           RowBox[{"16", ",", 
            RowBox[{"n", "-", "k"}], ",", 
            RowBox[{
             RowBox[{"8", "k"}], "+", "1"}]}], "]"}], ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"8", "k"}], "+", "1"}], ")"}]}], "]"}]}]}]}], "]"}], ";", 
    " ", "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", 
       RowBox[{"n", "+", "1"}]}], ",", 
      RowBox[{"k", "\[LessEqual]", "x"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s1", "=", 
       RowBox[{"s1", "+", 
        RowBox[{
         RowBox[{"16", "^", 
          RowBox[{"(", 
           RowBox[{"n", "-", "k"}], ")"}]}], "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"8", "k"}], "+", "1"}], ")"}]}]}]}]}], "]"}], ";", " ", 
    "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", "0"}], ",", 
      RowBox[{"k", "\[LessEqual]", "n"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s4", "=", 
       RowBox[{"s4", "+", 
        RowBox[{"Divide", "[", 
         RowBox[{
          RowBox[{"ExpModK", "[", 
           RowBox[{"16", ",", 
            RowBox[{"n", "-", "k"}], ",", 
            RowBox[{
             RowBox[{"8", "k"}], "+", "4"}]}], "]"}], ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"8", "k"}], "+", "4"}], ")"}]}], "]"}]}]}]}], "]"}], ";", 
    " ", "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", 
       RowBox[{"n", "+", "1"}]}], ",", 
      RowBox[{"k", "\[LessEqual]", "x"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s4", "=", 
       RowBox[{"s4", "+", 
        RowBox[{
         RowBox[{"16", "^", 
          RowBox[{"(", 
           RowBox[{"n", "-", "k"}], ")"}]}], "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"8", "k"}], "+", "4"}], ")"}]}]}]}]}], "]"}], ";", " ", 
    "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", "0"}], ",", 
      RowBox[{"k", "\[LessEqual]", "n"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s5", "=", 
       RowBox[{"s5", "+", 
        RowBox[{"Divide", "[", 
         RowBox[{
          RowBox[{"ExpModK", "[", 
           RowBox[{"16", ",", 
            RowBox[{"n", "-", "k"}], ",", 
            RowBox[{
             RowBox[{"8", "k"}], "+", "5"}]}], "]"}], ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"8", "k"}], "+", "5"}], ")"}]}], "]"}]}]}]}], "]"}], ";", 
    " ", "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", 
       RowBox[{"n", "+", "1"}]}], ",", 
      RowBox[{"k", "\[LessEqual]", "x"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s5", "=", 
       RowBox[{"s5", "+", 
        RowBox[{
         RowBox[{"16", "^", 
          RowBox[{"(", 
           RowBox[{"n", "-", "k"}], ")"}]}], "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"8", "k"}], "+", "5"}], ")"}]}]}]}]}], "]"}], ";", " ", 
    "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", "0"}], ",", 
      RowBox[{"k", "\[LessEqual]", "n"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s6", "=", 
       RowBox[{"s6", "+", 
        RowBox[{"Divide", "[", 
         RowBox[{
          RowBox[{"ExpModK", "[", 
           RowBox[{"16", ",", 
            RowBox[{"n", "-", "k"}], ",", 
            RowBox[{
             RowBox[{"8", "k"}], "+", "6"}]}], "]"}], ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"8", "k"}], "+", "6"}], ")"}]}], "]"}]}]}]}], "]"}], ";", 
    " ", "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", 
       RowBox[{"n", "+", "1"}]}], ",", 
      RowBox[{"k", "\[LessEqual]", "x"}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"s6", "=", 
       RowBox[{"s6", "+", 
        RowBox[{
         RowBox[{"16", "^", 
          RowBox[{"(", 
           RowBox[{"n", "-", "k"}], ")"}]}], "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"8", "k"}], "+", "6"}], ")"}]}]}]}]}], "]"}], ";", " ", 
    "\n", 
    RowBox[{"res", "=", 
     RowBox[{"Mod", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"4", "s1"}], "-", 
        RowBox[{"2", "s4"}], "-", "s5", "-", "s6"}], ",", "1"}], "]"}]}], ";",
     "\n", 
    RowBox[{"Return", "[", 
     RowBox[{"BaseForm", "[", 
      RowBox[{
       RowBox[{"N", "[", 
        RowBox[{"res", ",", "30"}], "]"}], ",", "16"}], "]"}], "]"}]}], "\n", 
   ")"}]}]], "Input",
